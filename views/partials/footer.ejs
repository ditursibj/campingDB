        // Used to show the modal on the userCamps page if a user has no associated campgrounds
        $('#noCampsModal').modal({show: true, backdrop: "static", keyboard: false});

        //Grow and shrink logos on hover
        $('.whiteLink')
          .on('mouseenter',  function() {
            var a = $(this);
            a.stop(true, true).animate({
              margin: "-.5rem",
              padding: "+.5rem",
              fontSize: "2.5rem",
              height: "+=20"
            }, 'fast');
        })
          .on('mouseleave', function(){
              var a = $(this);
              a.stop(true, true).animate({
